<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:schemaLocation="http://axelor.com/xml/ns/domain-models
  https://axelor.com/xml/ns/domain-models/domain-models_6.0.xsd">

    <module name="sales" package="com.axelor.sales.db"/>

    <entity name="Order">
        <string name="code" required="true" />
        <date name="orderDate" required="true" />
        <date name="confirmationDate"/>
        <decimal name="exTaxTotal" precision="8" scale="2" />
        <decimal name="total" precision="8" scale="2" />
        <one-to-many name="orderLineList" ref="com.axelor.contact.db.Contact"
        <integer name="stateSelect" />
    </entity>

</domain-models>